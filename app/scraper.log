$(document).ready(function(){
	$('.show_selected_numbe_products').change(function(){
		var productSingleElements = document.querySelectorAll('.product-single');
		var numbers=$(this).val();
		var initialItemsToShow = numbers;
		// Hide all elements
		for (var i = 0; i < productSingleElements.length; i++) {
			productSingleElements[i].style.display = 'none';
		}

		// Show the selected number of items
		for (var i = 0; i < numbers; i++) {
			if (i < productSingleElements.length) {
				
				productSingleElements[i].style.display = 'block';
			}
		}
		
		// Check if all items are displayed
		if (numbers >= productSingleElements.length) {
			// Hide the "Load More" button if all items are displayed
			document.getElementById('loadmoreten').style.display = 'none';
		} else {
			// Show the "Load More" button if not all items are displayed
			document.getElementById('loadmoreten').style.display = 'block';
		}
		
		
	});
	
	
var currentIndex = 0;
$('#loadmoreten').on('click', function () {
    var productSingleElements = document.querySelectorAll('.product-single');
    var itemsToShow = 20;
    showNextItems(itemsToShow);
    // Check if there are more items to show
    if (currentIndex >= productSingleElements.length) {
        // Hide the "Load More" button if all items are displayed
        document.getElementById('loadmoreten').style.display = 'none';
    }

    function showNextItems(itemsToShow) {
		var itemsToShow=itemsToShow;
        
        for (var i = currentIndex; i < currentIndex + itemsToShow; i++) {
            if (i < productSingleElements.length) {
                productSingleElements[i].style.display = 'block';
            }
        }
        currentIndex += itemsToShow;
    }
});
	
	
	
